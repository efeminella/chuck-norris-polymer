{
  "eslint-env": "es6",
  "parser": "babel-eslint",
  "env": {
    "browser": false,
    "node": true
  },
  "rules": {
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "semi": [2, "always"],
    "arrow-parens": [2, "always"],
    "prefer-arrow-callback": 2,
    "no-class-assign": 2
  }
}
